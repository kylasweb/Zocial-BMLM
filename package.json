{
  "name": "vite-react-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "postbuild": "cp public/_redirects dist/_redirects",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:report": "eslint . --ext .js,.jsx,.ts,.tsx -f json -o ./lint-report.json",
    "preview": "vite preview",
    "backup": "node scripts/backup.js",
    "audit": "node scripts/audit.js",
    "monitor": "node scripts/monitor.js",
    "security-check": "npm audit && snyk test",
    "update-deps": "npm-check-updates -u && npm install",
    "precommit": "npm run lint && npm run test",
    "analyze": "source-map-explorer 'dist/**/*.js'",
    "clean": "rimraf dist node_modules/.cache",
    "monitoring:setup": "node scripts/monitoring/setup.js",
    "monitoring:check": "node scripts/monitoring/healthcheck.js",
    "security:audit": "node scripts/security/audit.js",
    "backup:verify": "node scripts/backup/verify.js",
    "validate:all": "node scripts/validation/run-all.js",
    "env:validate": "node scripts/config/validate-env.js",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:unit": "vitest run",
    "test:integration": "vitest integration",
    "test:e2e": "cypress run",
    "performance:analyze": "node scripts/performance/analyze.js",
    "predeploy": "node scripts/pre-deploy.js",
    "deploy:staging": "npm run predeploy && node scripts/deploy.js staging",
    "deploy:prod": "npm run predeploy && node scripts/deploy.js production"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.24.1",
    "@codemirror/basic-setup": "^0.20.0",
    "@codemirror/lang-javascript": "^6.2.3",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@faker-js/faker": "^8.0.0",
    "@monaco-editor/react": "^4.7.0",
    "@mui/material": "^5.16.14",
    "@reduxjs/toolkit": "^2.6.0",
    "@sentry/node": "^9.4.0",
    "@sentry/react": "^9.4.0",
    "@sentry/tracing": "^7.120.3",
    "@tanstack/react-query": "^5.67.1",
    "@types/recharts": "^1.8.29",
    "antd": "^5.0.0",
    "blueprint": "^2.0.0",
    "codemirror": "^6.0.1",
    "crypto-js": "^4.1.1",
    "date-fns": "4.1.0",
    "dompurify": "^3.2.4",
    "echarts": "^5.5.0",
    "echarts-for-react": "^3.0.2",
    "ethers": "^6.11.1",
    "evergreen-ui": "^7.1.9",
    "express-mongo-sanitize": "^2.2.0",
    "framer-motion": "^11.18.2",
    "grommet": "^2.0.0",
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.0.4",
    "jotai": "^2.12.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "netlify-plugin-cache": "^1.0.3",
    "netlify-plugin-submit-sitemap": "^0.4.0",
    "react": "^18.3.1",
    "react-ace": "^14.0.1",
    "react-admin": "^4.14.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-color": "^2.19.3",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.3.1",
    "react-google-recaptcha": "^3.1.0",
    "react-i18next": "^15.4.1",
    "react-icons": "^5.5.0",
    "react-responsive": "^9.0.2",
    "react-router-dom": "^7.2.0",
    "react-toastify": "^11.0.5",
    "recharts": "^2.15.1",
    "semantic-ui-react": "^2.0.0",
    "uuid": "^9.0.0",
    "xss-clean": "^0.1.4",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@netlify/plugin-lighthouse": "^5.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "netlify-plugin-cache": "^1.0.3",
    "netlify-plugin-submit-sitemap": "^0.4.0",
    "postcss": "^8.4.49",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.17",
    "vite": "^6.2.0"
  },
  "engines": {
    "node": ">=18.0.0 <19.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
